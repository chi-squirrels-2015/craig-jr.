<!DOCTYPE html>
<html>
<head>
  <title>CraigslistJr</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <!--   <header> -->
  <header>
    <ul id="nav_left">
      <li>
        <%= link_to 'Home', root_path %>
      </li>
    </ul>
    <ul id="nav_right">
      <li>
<% if current_user %>
  <%= link_to 'Profile', user_path(current_user) %> <%= link_to "Logout", '/logout' %>
<% else %>
  <%= link_to 'Login', '/login' %> <%= link_to 'Signup', '/signup' %>
<% end %>

      </li>
    </ul>
  </header>
  <!-- </header> -->
<%= yield %>

</body>
</html>
